{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "semi": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<TYPES>",
    "",
    "^(react/(.*)$)|^(react$)|^(react-native(.*)$)",
    "^(next/(.*)$)|^(next$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>^@acme",
    "^@acme/(.*)$",
    "",
    "<TYPES>^[.|..|~]",
    "^@/payload-types/(.*)$",
    "^~/",
    "^[../]",
    "^[./]",
    "^@/lib/(.*)$",
    "^@/utilities/(.*)$",
    "^@/components/(.*)$",
    "^@/hooks/(.*)$",
    "^@/store/(.*)$",
    "^@/styles/(.*)$",
    "^@/types/(.*)$",
    "^@/blocks/(.*)$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
