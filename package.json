{
  "name": "create-cronex-app",
  "version": "0.1.0",
  "description": "Create a new Payload CMS + Next.js project with the Cronex template",
  "type": "module",
  "bin": {
    "create-cronex-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "typecheck:extras": "tsc --project template/extras/tsconfig.json --noEmit",
    "typecheck:all": "pnpm run typecheck && pnpm run typecheck:extras",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "payload",
    "cms",
    "nextjs",
    "template",
    "cli",
    "create-app"
  ],
  "author": "CRONEXi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CRONEXi/create-cronex-app"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@payloadcms/db-mongodb": "^3.0.0",
    "@payloadcms/db-postgres": "^3.0.0",
    "@payloadcms/db-sqlite": "^3.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.29",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "better-auth": "^1.2.0",
    "better-sqlite3": "^11.0.0",
    "mongodb": "^6.3.0",
    "next": "^15.0.0",
    "payload": "^3.0.0",
    "pg": "^8.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^2.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "zod": "^3.23.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
